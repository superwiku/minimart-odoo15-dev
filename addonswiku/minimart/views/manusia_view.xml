<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- minimart.manusia tree view -->
    <record id="minimart_manusia_view_tree" model="ir.ui.view">
        <field name="name">minimart.manusia.view.tree</field>
        <field name="model">minimart.manusia</field>
        <field name="mode">primary</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="gender"/>
                <field name="alamat"/>                
            </tree>
        </field>
    </record>

    <!-- minimart.manusia form view -->
    <record id="minimart_manusia_view_form" model="ir.ui.view">
        <field name="name">minimart.manusia.view.form</field>
        <field name="model">minimart.manusia</field>
        <field name="mode">primary</field>
        <field name="arch" type="xml">
            <form string="">
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="gender"/>
                        <field name="alamat"/>                        
                    </group>
                </sheet>
            </form>
        </field>
    </record>

</odoo>
